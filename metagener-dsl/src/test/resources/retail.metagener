// define a data type of item_scan, with 10E9 unique samples
entity item_scan pop=1000000000

 // create a timestamp, advancing by sampleid * 1000 (1000 millis)
 //  with the beginning starting in year 2015
 field scantime : timestamp <- multiply(1000); dateshift(2015);

 // Hash the sampleid, sample from product_ids with uniform distribution
 field product_id:text      <- murmur3(); filesample(/cache/product_ids.txt,uniform);

entity retail.stores.phones
  field phone_number : text <- three=range(111,999); four=range(1111,9999); template("{{three()}}-{{three()}}-{{four()}}");

entity retail.stores pop=1000
  field store_id : int
  field tax_rage : decimal

  field express_registers : int
  field full_registers: int

  field street : text
  field city : text
  field zip_code : text
  field phones: map <- buildmap("{{range(111,999)}}-{{range(111,999)}}-{{range(1111,9999)}},range(1,3))





// create a default sampler for item_scan
sampler item_scan