entity user pop=1000
 field user_id int
 // free func with just a name
 func usertype testbasicfuncname;
 // field func with malformed params
 field firstlastname text func=entity;template("${firstname} ${lastname}");
// A random comment here
 field username text func=entity;sample(file=usernames.csv,dist=uniform);
 field join_date timestamp func=entity;datetime(range=1990-1993,dist=binomial); // another random comment
 field firstname text func =entity;sample(file=firstnames.csv,dist=uniform);
 field lastname text func= entity;sample(file=lastnames.csv,dist=uniform);

entity message // no pop -- means Int.MAX_VALUE
 field sender_id int func=entity;dist:binomial
 field recip_id int func=entity;dist:binomial
 field sent_at timestamp func=stream;datetime(range=1990-1993,dist=pareto);

// sampler with name=message entity=message, function=<default>
sampler message
// sampler with name=message-sampler, entity=message, function=<default>
sampler message-sampler:message
//sampler with name=message-sampler, entity=message, function=dist:pareto
sampler message-sampler:message dist(pareto)

